{
  "name": "@iron/extension",
  "private": true,
  "version": "0.0.1",
  "description": "A dev-oriented web3 wallet",
  "license": "MIT",
  "scripts": {
    "build": "yarn clean && parcel build manifest.json --no-content-hash",
    "build:zip": "yarn build && zip -r extension.zip ./dist",
    "dev": "parcel watch manifest.json --no-cache --host localhost",
    "clean": "rm -rf dist .cache",
    "bundle-analyzer": "yarn clean && parcel build manifest.json --reporter @parcel/reporter-bundle-analyzer && google-chrome-stable parcel-bundle-reports/default.html",
    "lint": "eslint ."
  },
  "dependencies": {
    "@metamask/eth-json-rpc-middleware": "https://github.com/MetaMask/eth-json-rpc-middleware#e7a1de5cc9c76f24f436be69850d442c46582975",
    "@metamask/object-multiplex": "^1.2.0",
    "@metamask/post-message-stream": "^6.1.0",
    "@parcel/config-webextension": "^2.8.3",
    "@parcel/reporter-bundle-analyzer": "^2.8.3",
    "eth-json-rpc-filters": "^6.0.0",
    "eth-rpc-errors": "^4.0.3",
    "extension-port-stream": "^2.0.1",
    "fast-deep-equal": "^3.1.3",
    "is-stream": "^3.0.0",
    "json-rpc-engine": "^6.1.0",
    "json-rpc-middleware-stream": "^4.2.1",
    "parcel": "^2.8.3",
    "pump": "^3.0.0",
    "stream": "^0.0.2",
    "stream-browserify": "^3.0.0",
    "typescript": "^4.9.5",
    "webextension-polyfill": "^0.10.0",
    "websocket-stream": "^5.5.2",
    "websocket-ts": "^1.1.1"
  },
  "devDependencies": {
    "@types/pump": "^1.1.1",
    "@types/webextension-polyfill": "^0.10.0",
    "buffer": "^5.5.0",
    "events": "^3.1.0",
    "process": "^0.11.10"
  }
}
