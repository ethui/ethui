{
  "name": "@ethui/extension",
  "private": true,
  "version": "1.10.1",
  "description": "An Ethereum toolkit",
  "homepage": "https://github.com/ethui/ethui",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "yarn dev:chrome & yarn dev:firefox",
    "dev:chrome": "tsx ./scripts/build.ts --target chrome-dev",
    "dev:firefox": "tsx ./scripts/build.ts --target firefox-dev",
    "watch:chrome": "tsx watch --include src --include manifest --include 'vite/*.ts' --include package.json ./scripts/build.ts --target chrome-dev",
    "watch:firefox": "tsx watch --include src --include manifest --include 'vite/*.ts' --include package.json ./scripts/build.ts --target firefox-dev",
    "build": "tsx ./scripts/build.ts \"$@\""
  },
  "dependencies": {
    "@devtools-ds/object-inspector": "^1.2.1",
    "@devtools-ds/table": "^1.2.1",
    "@lukeed/uuid": "^2.0.1",
    "@metamask/eth-json-rpc-middleware": "^15.1.2",
    "@metamask/json-rpc-engine": "^10.0.2",
    "@metamask/json-rpc-middleware-stream": "^8.0.6",
    "@metamask/post-message-stream": "^9.0.0",
    "@metamask/utils": "^11.1.0",
    "eth-json-rpc-filters": "^6.0.1",
    "eth-rpc-errors": "^4.0.3",
    "eventemitter3": "^5.0.1",
    "loglevel": "^1.9.2",
    "mipd": "^0.0.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "stream": "^0.0.3",
    "webextension-polyfill": "^0.12.0",
    "websocket-ts": "^2.1.5",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.301",
    "@types/json-merge-patch": "^1.0.0",
    "@types/node": "^22.13.1",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/webextension-polyfill": "^0.12.1",
    "@types/yargs": "^17.0.33",
    "autoprefixer": "^10.4.20",
    "crx": "^5.0.1",
    "json-merge-patch": "^1.0.2",
    "json5": "^2.2.3",
    "postcss": "^8.5.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "5.7.3",
    "vite": "^6.0.11",
    "vite-plugin-node-polyfills": "^0.23.0",
    "vite-tsconfig-paths": "^5.1.4",
    "web-ext": "^8.3.0",
    "yargs": "^17.7.2"
  },
  "resolutions": {
    "rollup": "4.22.4"
  }
}
