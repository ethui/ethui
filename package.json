{
  "private": true,
  "version": "1.9.0",
  "license": "MIT",
  "description": "An Ethereum toolkit",
  "scripts": {
    "dev": "pnpm run app:dev",
    "app:dev": "pnpm tauri dev --config bin/tauri.dev.conf.json  --features ${ETHUI_FEATURES:-''} --target $(pnpm run rust-target | tail -n1)",
    "app:build": "pnpm tauri build --target $(pnpm run rust-target | tail -n1)",
    "app:build:test": "pnpm tauri build --debug --no-bundle --config bin/tauri.test.conf.json --features test",
    "rust-target": "rustc -vV | sed -n 's/host: //p'",
    "gui:build": "pnpm --filter @ethui/gui build",
    "gui:devtools": "pnpm --filter @ethui/gui devtools",
    "lint": "pnpm lint:biome && pnpm lint:tsc && pnpm --filter @ethui/gui run lint:unused",
    "lint:biome": "biome check .",
    "lint:tsc": "pnpm --recursive tsc --noEmit",
    "fix": "pnpm fix:biome",
    "fix:biome": "biome check --write .",
    "test": "pnpm run test:integration",
    "test:integration": "pnpm --filter @ethui/webdriverio test"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.2",
    "@tailwindcss/postcss": "^4.1.3",
    "ts-unused-exports": "^11.0.1"
  },
  "dependencies": {
    "@ethui/ui": "^0.0.92",
    "@tauri-apps/cli": "^2.9.2",
    "typescript": "5.9.2"
  },
  "engines": {
    "node": ">=24.0.0"
  },
  "resolutions": {
    "react-hook-form": "7.59.0",
    "esbuild": "0.25.2"
  },
  "packageManager": "pnpm@9.4.0+sha512.f549b8a52c9d2b8536762f99c0722205efc5af913e77835dbccc3b0b0b2ca9e7dc8022b78062c17291c48e88749c70ce88eb5a74f1fa8c4bf5e18bb46c8bd83a"
}
