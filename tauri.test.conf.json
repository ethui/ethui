{
  "$schema": "https://schema.tauri.app/config/2.0.0",
  "productName": "ethui-test",
  "identifier": "dev.ethui.test",
  "build": {
    "beforeDevCommand": "pnpm --filter @ethui/gui build",
    "beforeBuildCommand": "pnpm --filter @ethui/gui build",
    "frontendDist": "../gui/dist",
    "devUrl": "http://localhost:1420"
  },
  "app": {
    "security": {
      "csp": {
        "default-src": "'self' 'unsafe-inline' 'unsafe-eval'",
        "font-src": ["'self'", "data:"],
        "style-src": ["'self'", "'unsafe-inline'"],
        "connect-src": ["*"],
        "img-src": ["'self'", "data:", "*"]
      }
    },
    "withGlobalTauri": true,
    "windows": [
      {
        "label": "main",
        "title": "ethui Test",
        "width": 1200,
        "height": 800,
        "resizable": true,
        "fullscreen": false,
        "decorations": true,
        "alwaysOnTop": false,
        "skipTaskbar": false,
        "center": true
      }
    ]
  },
  "bundle": {
    "active": false
  },
  "plugins": {
    "updater": {
      "active": false
    }
  }
}